@model IEnumerable<MathProject.Models.Question>

@{
    ViewData["Title"] = "ChooseToEdit";
}

<h2>Wybierz pytanie które chcesz edytować</h2>

@foreach(var item in Model)
{
    <a class="btn btn-tile-task" id="more" asp-action="ChooseHowToEdit" asp-route-id="@item.ID">@Html.DisplayFor(modelItem=> item.Content) </a>

}
<div id="checked">

</div>
      