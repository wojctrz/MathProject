
@model MathProject.Models.Question

@{
    ViewData["Title"] = "Wybierz sposób rozwiązywania";
}
<body>

    <h2>Jak chcesz rozwiązać to zadanie?</h2>

    <a class="btn btn-tile" asp-action="Solve" asp-route-id="@Model.ID"><span class="inside-button">Rozwiązuję sam</span> </a>
    <a class="btn btn-tile-task" asp-action="SolveWithHints" asp-route-questionID="@Model.ID" asp-route-hintID="0"><span class="inside-button">Rozwiązuję z podpowiedziami </span> </a>
  
</body>